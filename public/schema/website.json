{
  "@context": "https://schema.org",
  "@type": "WebSite",
  "@id": "https://ingredient-substituter.vercel.app/#website",
  "url": "https://ingredient-substituter.vercel.app/",
  "name": "Ingredient Substituter",
  "description": "An online tool to find ingredient substitutions for cooking and baking.",
  "publisher": {
    "@type": "Organization",
    "name": "Ingredient Substituter",
    "url": "https://ingredient-substituter.vercel.app/"
  },
  "potentialAction": {
    "@type": "SearchAction",
    "target": {
      "@type": "EntryPoint",
      "urlTemplate": "https://ingredient-substituter.vercel.app/?ingredient={search_term}"
    },
    "query-input": "required name=search_term"
  }
}